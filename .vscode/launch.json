  "version": "0.2.0",
  "configurations": [
    {
      "name": "mama",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define",
        "API_URL=https://api.mama-api.ru/api/v1/",
        "--dart-define",
        "ENVIRONMENT=DEV"
      ]
    },
    {
      "name": "mama (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--dart-define",
        "API_URL=https://api.mama-api.ru/api/v1/",
        "--dart-define",
        "ENVIRONMENT=DEV"
      ]
    },
    {
      "name": "mama (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--dart-define",
        "API_URL=https://api.mama-api.ru/api/v1/",
        "--dart-define",
        "ENVIRONMENT=DEV"
      ]
    }
  ]
}
